let magicians = ["Calvin", "Martin", "Andy", "Kevin"];
function show_magicians(magiciansNames) {
  magiciansNames.map((item) => {
    console.log(item);
  });
}
show_magicians(magicians);

function make_great() {
  for (let i = 0; i < magicians.length; i++) {
    magicians[i] = `The Great ${magicians[i]}`;
  }
}
make_great();
show_magicians(magicians);